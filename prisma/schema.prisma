// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Passenger {
  id        Int    @id @default(autoincrement())
  firstName String @unique
  lastName  String
}

model User {
  id          Int    @id @default(autoincrement())
  email       String @unique
  password    String
  name        String
  birthdate   DateTime
  gender      String
  country     String
  university  String
  department  String
  studentId   String
  createdAt   DateTime @default(now())

  
  @@map("users")
}
